[
  {
    "type": "escape",
    "width": 80,
    "align": "left"
  },

  {
    "type": "volume",
    "width": 100,
    "align": "left",
    "image": {
      "base64": ""
      },
  },

  // Convert script into line with escate quotes
  // https://www.freeformatter.com/javascript-escape.html#ad-output

  // Wi-Fi
  {
    "type": "appleScriptTitledButton",
    // "source": { "filePath": "~/applescripts/status_wifi.scpt" },
    "source": { "inline": "set device to do shell script \"networksetup -listallhardwareports | awk '$3==\\\"Wi-Fi\\\" {getline;print}' | awk '{print $2}'\"\r\n\r\nset power to do shell script \"networksetup -getairportpower \" & device & \" | awk '{print $4}'\"\r\n\r\nreturn {power, power}" },
    "refreshInterval": 2,
    "action": "appleScript",
    // "actionAppleScript": { "filePath": "~/applescripts/toggle_wifi.scpt" },
    "actionAppleScript": { "inline": "set device to do shell script \"networksetup -listallhardwareports | awk '$3==\\\"Wi-Fi\\\" {getline;print}' | awk '{print $2}'\"\r\n\r\nset power to do shell script \"networksetup -getairportpower \" & device & \" | awk '{print $4}'\"\r\n\r\nif power is equal to \"on\" then\r\n\tset power to \"off\"\r\nelse\r\n\tset power to \"on\"\r\nend if\r\n\r\ndo shell script (\"networksetup -setairportpower \" & device & \" \" & power)" },
    "bordered": true,
    "width": 50,
    "align": "right",
    "image": {
        "base64": ""
    },
    "alternativeImages": {
        "On": {
            "base64": ""
        },
        "Off": {
            "base64": ""
        }
    }
  },

  { "type": "displaySleep", "width": 40, "align": "right", "bordered": false },

  // ScreenShot
  // copy of touchbar - key code 22
  {
    "type": "staticButton",
    "title": "",
    "action": "appleScript",
        "bordered": true,
        "align": "right",
    "actionAppleScript": {
      "inline": "tell application \"System Events\"\nkey code 21 using {command down, shift down, control down} \rend tell"
    }, 
    "longAction": "appleScript",
        "longActionAppleScript": {
            "inline": "tell application \"System Events\"\nkey code 23 using {command down, shift down} \rend tell"
        },
    "image": {
      "base64": ""
    }
  },
]
